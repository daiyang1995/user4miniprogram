.bodyContent {
  background-color: #fff;
  font-size: 28rpx;
}

::-webkit-input-placeholder {
  /* WebKit browsers */
  font-size: 28rpx;
  color: #e4e4e4;
}

:-moz-placeholder {
  /* Mozilla Firefox 4 to 18 */
  font-size: 28rpx;
  color: #e4e4e4;
}

::-moz-placeholder {
  /* Mozilla Firefox 19+ */
  font-size: 28rpx;
  color: #e4e4e4;
}

:-ms-input-placeholder {
  /* Internet Explorer 10+ */
  font-size: 28rpx;
  color: #e4e4e4;
}

a {
  color: #3385ff !important;
  text-decoration: none !important;
  font-weight: 500 !important;
}

input {
  font-size: 28rpx;
  width: 100%;
  border: none;
  outline: none;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  color: #b2b2b2;
  padding-left: 20rpx;
}

.borderLeft {
  border-left: 2rpx solid #c8c7cd;
}

.marginTop {
  margin-top: 50rpx;
}

.textAlignCenter {
  text-align: center;
}

.fullIcon:before {
  font-family: fullIconAll;
  color: #b2b2b2;
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
  width: 80rpx;
  float: left;
  text-align: center;
  overflow: hidden;
}

.headerIcon:before {
  content: "\f2be";
  font-size: 60rpx;
}

.idCardIcon:before {
  content: " ";
  width: 50rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100%;
  background-position: center center;
  background-repeat: no-repeat;
}

.mobileIcon:before {
  content: " ";
  width: 50rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100%;
  background-position: center center;
  background-repeat: no-repeat;
}

.identifyingCodeIcon:before {
  content: " ";
  width: 50rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100%;
  background-position: center center;
  background-repeat: no-repeat;
}

.bodyContent .oneline:after {
  content: " ";
  display: block;
  height: 0;
  visibility: hidden;
  clear: both;
}

.bodyContent .oneline>input {
  display: inline-block;
  box-sizing: border-box;
  margin-left: 10rpx;
  width: calc(100% - 60rpx);
  float: left;
  border-radius: 0 !important;
  border-bottom: 2rpx solid #c8c7cd;
}

.bodyContent .oneline>.ui-grid-a {
  margin-left: 60rpx;
  border-bottom: 2rpx solid #c8c7cd;
}

.bodyContent .oneline>.ui-grid-a>.ui-block-a {
  width: 60%;
}

.bodyContent .oneline>.ui-grid-a>.ui-block-b {
  width: 40%;
}
.bodyContent .oneline.identifyingCodeIcon>view{
  width: calc(100% - 60rpx);
}

.bodyContent .oneline .sendCode {
  height: 60rpx;
  line-height: 60rpx;
  margin: 10rpx;
  color: #3385ff;
  width: 100%;
  box-sizing: border-box;
  float: right;
}

.bodyContent .imgview {
  padding: 100rpx 0 20rpx 0;
  display: block;
  background-color: #fff;
}

.bodyContent .imgview>img {
  width: 100%;
  display: block;
}

.bodyContent .baseInfo {
  margin: 0 70rpx;
}

.bodyContent .bindingBtn {
  background-color: #3385ff;
  color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  margin-top: 70rpx;
  font-size: 28rpx;
}

.bodyContent .tel {
  color: #3385ff;
  height: 80rpx;
  line-height: 80rpx;
}
